{
  "name": "@seikonn/vueuse",
  "type": "module",
  "version": "0.0.1",
  "description": "collection of my utilities",
  "author": "seikonn <https://github.com/seikonn>",
  "license": "MIT",
  "homepage": "https://github.com/seikonn/vueuse#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/seikonn/vueuse"
  },
  "keywords": [
    "vue",
    "vue-use",
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/lib/index.js",
      "import": "./dist/es/index.js",
      "types": "./dist/es/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "unpkg": "./index.iife.js",
  "jsdelivr": "./index.iife.js",
  "types": "dist/es/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm run clean && rollup --config rollup.config.ts --configPlugin typescript",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^24.3.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.22.4",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.8.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {}
}